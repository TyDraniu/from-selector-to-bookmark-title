{
  "title": "DOM element as a bookmark",
  "name": "post-number",
  "version": "0.1.0",
  "description": "Reading DOM element and saving it as bookmark title",
  "main": "index.js",
  "author": "TyDraniu",
  "engines": {
    "firefox": ">=38.0a1"
  },
  "license": "MIT",
      "preferences": [
	{
        "description": "Your URL with source data",
        "name": "sourceURL",
        "type": "string",
        "value": "http://mozillapl.org/forum/index.php",
        "title": "Source URL"
    },
	{
        "description": "Page selector to display on the Bookmarks Toolbar",
        "name": "selector",
        "type": "string",
        "value": "#page-body > P:last-child > STRONG:first-child",
        "title": "Selector"
    },
	{
        "description": "Number of seconds between checks (min = 20s)",
        "name": "interval",
        "type": "integer",
        "value": 60,
        "title": "Number of seconds"
    }
	]
}
