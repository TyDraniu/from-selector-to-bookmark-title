{
  "background": {
      "scripts": ["background.js"]
  },
  "description": "Reading DOM element and saving it as a bookmark title",
  "homepage_url": "https://github.com/TyDraniu/",
  "manifest_version": 2,
  "name": "DOM element as a bookmark",
  "options_ui": {
      "page": "options.html",
      "browser_style": true
  },
  "permissions": [
    "<all_urls>",
	"storage", 
	"bookmarks",
	"alarms"],
  "version": "1.0.1",
  "applications": {
      "gecko": {
          "id": "@post-number",
          "strict_min_version": "57.0a1"
      }
  }
}
